<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <applicationSettings>
    <HandbrakeBatchEncoder.Properties.Settings>
      <setting name="WatchFolder" xdt:Locator="Match(name)" serializeAs="String" xdt:Transform="Replace">
        <value>D:\Encode\Drop</value>
      </setting>
      <setting name="OutputFolder" xdt:Locator="Match(name)" serializeAs="String" xdt:Transform="Replace">
        <value>D:\Encode\Encoded</value>
      </setting>
    </HandbrakeBatchEncoder.Properties.Settings>
  </applicationSettings>
</configuration>